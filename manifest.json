{
  "name": "jLooker",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "English to Japanese for menus in Looker",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "webRequest",
    "https://*.looker.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.looker.com/*"
      ],
      "js": [
        "js/underscore/underscore.js",
        "src/inject/inject.js"
      ]
    }
  ]
}
